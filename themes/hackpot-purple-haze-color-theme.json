    "tokenColors": [
        {
            "name": "Magenta",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "markup.quote.markdown"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#990099"
            }
        },
        {
            "name": "Red Orange",
            "scope": [
                "markup.deleted",
                "support.type.exception",
                "token.error-token"
            ],
            "settings": {
                "foreground": "#ff661a"
            }
        },
        {
            "name": "Light Red",
            "scope": [
                "variable.language",
            ],
            "settings": {
                "foreground": "#ff704d"
            }
        },
        {
            "name": "Bright Green",
            "scope": [
                "entity.name.function",
                "entity.name.tag",
                "meta.function-call",
                "entity.name.type",
                "meta.embedded.block.html",
                "variable.other.global",
                "string.other.link.description.markdown",
                "variable.other.property",
                "keyword.other.unit",
                "markup.list",
                "string.other.link.title"
            ],
            "settings": {
                "foreground": "#5cd65c"
            }
        },
        {
            "name": "Sea Green",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#00994d"
            }
        },
        {
            "name": "Blue",
            "scope": [
                "support"
            ],
            "settings": {
                "foreground": "#50b4fb"
            }
        },
        {
            "name": "Fog Blue",
            "scope": [
                "meta.function.url",
                "markup.underline.link",
                "meta.link.reference",
                "variable.parameter.url"
            ],
            "settings": {
                "foreground": "#7ebbe7"
            }
        },
        {
            "name": "Light Purple",
            "scope": [
                "constant.language",
                "entity.name.section.markdown",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#cc99ff"
            }
        },
        {
            "name": "Pink",
            "scope": [
                "constant.numeric",
                "constant.character.numeric"
            ],
            "settings": {
                "foreground": "#e56697"
            }
        },
        {
            "name": "Aqua Blue",
            "scope": [
                "string.quoted",
                "punctuation.definition.string",
                "markup.fenced_code.block.markdown",
                "string.interpolated",
                "string.template",
                "markup.inline.raw.string"
            ],
            "settings": {
                "foreground": "#1fada6"
            }
        },
        {
            "name": "Red",
            "scope": [
                "constant.character.escape",
                "constant.other.placeholder",
                "constant.other.character-class",
                "constant.character.entity",
                "keyword.operator.new",
                "keyword.operator.ternary",
                "keyword.operator.quantifier",
                "keyword.operator.negation",
                "keyword.operator.unpacking",
                "keyword.operator.arithmetic",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "keyword.operator.channel",
                "keyword.operator.redirect",
                "keyword.operator.expansion",
                "keyword.operator.spread",
                "keyword.operator.rest",
                "keyword.operator.address",
                "keyword.operator.ellipsis",
                "punctuation.vararg-ellipses",
                "meta.separator.markdown",
                "markup.inserted.math.display.markdown",
            ],
            "settings": {
                "foreground": "#f1414a"
            }
        },
        {
            "name": "Purple",
            "scope": [
                "keyword",
                "storage.type.js",
                "storage.type.function",
                "entity.other",
                "support.type.property-name",
                "entity.name.tag.localname",
                "entity.other.attribute-name.html",
                "markup.raw.block.markdown"
            ],
            "settings": {
                "foreground": "#a64dff"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "punctuation",
                "keyword.operator",
                "meta.brace",
                "constant.character",
                "markup.heading.setext",
                "punctuation.definition.list",
                "storage.type.function.arrow",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#ffff66"
            }
        },
        {
            "name": "Very Light Purple",
            "scope": [
                "meta.embedded.block",
                "meta.assertion",
                "meta.group.assertion",
                "meta.function-call.arguments",
                "variable.other",
                "variable.other.property",
                "entity.other.attribute-name",
                "string.unquoted.html",
                "meta.object",
                "constant.other",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#f2e6ff"
            }
        },
        {
            "name": "Light Orange",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#ffcc80"
            }
        }
    ]
}