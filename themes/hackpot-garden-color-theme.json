	"tokenColors": [
		{
			"name": "Dark Green",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"markup.quote.markdown"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#1e621e"
			}
		},
		{
			"name": "Red Orange",
			"scope": [
				"markup.deleted",
				"support.type.exception",
