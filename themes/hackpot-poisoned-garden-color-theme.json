    "tokenColors": [
        {
            "name": "Dark Green",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "markup.quote.markdown"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#1e621e"
            }
        },
        {
            "name": "Red Orange",
            "scope": [
                "markup.deleted",
                "support.type.exception",
                "token.error-token"
            ],
            "settings": {
                "foreground": "#ff661a"
            }
        },
        {
            "name": "Light Red",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#ff704d"
            }
        },
        {
            "name": "Bright Green",
            "scope": [
                "entity.name.function",
                "entity.name.tag",
                "meta.function-call",
                "entity.name.type",
                "meta.embedded.block.html",
                "variable.other.global",
                "string.other.link.description.markdown",
                "variable.other.property",
                "keyword.other.unit",
                "markup.list",
                "string.other.link.title"
            ],
            "settings": {
                "foreground": "#32CD32"
            }
        },
        {
            "name": "Tan",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#d0a97c"
            }
        },
        {
            "name": "Sea Green",
            "scope": [
                "support"
            ],
            "settings": {
                "foreground": "#00b359"
            }
        },
        {
            "name": "Blue",
            "scope": [
                "meta.function.url",
                "markup.underline.link",
                "meta.link.reference",
                "variable.parameter.url"
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Purple",
            "scope": [
                "constant.language",
                "entity.name.section.markdown",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#b366ff"
            }
        },
        {
            "name": "Pink",
            "scope": [
                "constant.numeric",
                "constant.character.numeric"
            ],
            "settings": {
                "foreground": "#e56697"
            }
        },
        {
            "name": "Aqua Blue",
            "scope": [
                "string.quoted",
                "punctuation.definition.string",
                "markup.fenced_code.block.markdown",
                "string.interpolated",
                "string.template",
                "markup.inline.raw.string"
            ],
            "settings": {
                "foreground": "#1fada6"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "constant.character.escape",
                "constant.other.placeholder",
                "constant.other.character-class",
                "constant.character.entity",
                "keyword.operator.new",
                "keyword.operator.ternary",
                "keyword.operator.quantifier",
                "keyword.operator.negation",
                "keyword.operator.unpacking",
                "keyword.operator.arithmetic",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "keyword.operator.channel",
                "keyword.operator.redirect",
                "keyword.operator.expansion",
                "keyword.operator.spread",
                "keyword.operator.rest",
                "keyword.operator.address",
                "keyword.operator.ellipsis",
                "punctuation.vararg-ellipses",
                "meta.separator.markdown",
                "markup.inserted.math.display.markdown"
            ],
            "settings": {
                "foreground": "#ffff4d"
            }
        },
        {
            "name": "Brown",
            "scope": [
                "keyword",
                "storage.type.js",
                "storage.type.function",
                "entity.other",
                "support.type.property-name",
                "entity.name.tag.localname",
                "entity.other.attribute-name.html",
                "markup.raw.block.markdown"
            ],
            "settings": {
                "foreground": "#9F6934"
            }
        },
        {
            "name": "Orange",
            "scope": [
                "punctuation",
                "keyword.operator",
                "meta.brace",
                "constant.character",
                "markup.heading.setext",
                "punctuation.definition.list",
                "storage.type.function.arrow",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#ffb84d"
            }
        },
        {
            "name": "Light Green",
            "scope": [
                "meta.embedded.block",
                "meta.assertion",
                "meta.group.assertion",
                "meta.function-call.arguments",
                "variable.other",
                "variable.other.property",
                "entity.other.attribute-name",
                "string.unquoted.html",
                "meta.object",
                "constant.other",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#b1e7b1"
            }
        },
        {
            "name": "Mint",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#76d576"
            }
        }
    ]
